            <property name="queryText"> SELECT 
  	CASE WHEN solm.observaciones_generales IS NOT NULL AND solm.observaciones_generales&lt;>'' THEN 'Indicaciones Generales: '|| solm.observaciones_generales else '' end as observaciones,
   desp.fecha  as fecha, 'Nombre y Apellidos de quien despacha: '|| getnombrepersona(usu.codigo_persona) as despachador,
  getidentificacionpersona(usu.codigo_persona) as id, getacronimotipodocumento(gettipoidentificacionpersona(usu.codigo_persona)) as acro,
 CASE WHEN desp.numero_solicitud IS NOT NULL THEN 'Establecimiento Farmaceutico Minorista: '||cen.descripcion ELSE '' END as razonsocial ,CASE WHEN desp.numero_solicitud IS NOT NULL THEN  ', Direccion: '|| cen.direccion ELSE '' END as direccion, 
 CASE WHEN desp.numero_solicitud IS NOT NULL THEN 'Fecha Despacho: ' ELSE '' END as probf, CASE WHEN desp.numero_solicitud IS NOT NULL THEN 'Despacho Medicamentos ' ELSE '' END as existe
 from 
  solicitudes_medicamentos solm
  inner join ORDENES.solicitudes sol on (sol.numero_solicitud=solm.numero_solicitud)
  inner join ADMINISTRACION.centros_costo cco on (cco.codigo= sol.centro_costo_solicitante)
  inner join ADMINISTRACION.centro_atencion cen on ( cco.centro_atencion = cen.codigo )
  left join INVENTARIOS.despacho desp ON (desp.numero_solicitud = solm.numero_solicitud)
  left join usuarios usu ON (desp.usuario=usu.login)
 where 
 	numero_solicitud=?</property>
