            <property name="queryText">select
'Paciente: '|| getnombrepersona(c.codigo_paciente) ||', '||p.tipo_identificacion ||' '|| p.numero_identificacion||	', Edad: '|| getedad2(p.fecha_nacimiento, s.fecha_solicitud)||', Sexo: '|| (CASE WHEN p.sexo=1 THEN 'Masculino' ELSE 'Femenino' END) as nombre_paciente,
    'Nro.Historia Clíinica: '||pa.historia_clinica||', Nro.Orden: '||s.consecutivo_ordenes_medicas ||', Fecha-Hora: '|| to_char(s.fecha_solicitud, 'DD/MM/YYYY') ||' '|| s.hora_solicitud as info_pac,
	'Dirección: '|| p.direccion ||', Tel: '|| coalesce(p.telefono, '') ||', Ciudad: '|| m.descripcion ||', Departamento:'||d.descripcion as "info_orden",
	'Afiliacion al S.G.S.S.S: ' || r.nombre||', Nombre Entidad: '||n.nombre as "lugar_prescripcion", 
     'Profesional que Pres-escribe: '|| getnombremedico(s.codigo_medico) ||', Especialidad: '||getnombreespecialidad(s.especialidad_solicitante) as "info_medico"
    	 from      solicitudes s 	inner join cuentas c on(c.id=s.cuenta) 	
     	inner join personas p on(p.codigo= c.codigo_paciente) 
     	inner join pacientes pa on (pa.codigo_paciente=p.codigo)
		inner join departamentos d on (d.codigo_departamento=p.codigo_departamento_vivienda)
		inner join ciudades m on (m.codigo_ciudad=p.codigo_ciudad_vivienda AND m.codigo_departamento=p.codigo_departamento_vivienda)
		inner join sub_cuentas sc on (c.id_ingreso=sc.ingreso)
		inner join convenios n on(n.codigo=sc.convenio) 
		inner join estratos_sociales e on(sc.clasificacion_socioeconomica= e.codigo)
		inner join tipos_regimen r on(r.acronimo=e.tipo_regimen)
	
where
	s.numero_solicitud=?</property>
        </oda-data-set>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="medicamentos" id="69">
            <structure name="cachedMetaData"/>
