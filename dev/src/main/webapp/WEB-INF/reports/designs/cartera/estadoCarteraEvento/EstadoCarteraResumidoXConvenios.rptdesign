            <property name="queryText">SELECT 
t.descripcion AS nombreempresa, 
c.nombre AS nombreconvenio, 
ec.descripcion as nombreestado, 
coalesce(sum(cc.valor_inicial_cuenta),0)*1 as valorinicial, 
coalesce(sum(cc.ajuste_debito),0)*1 as ajustedebito, 
coalesce(sum(cc.ajuste_credito),0)*1 as ajustecredito, 
coalesce(sum(cc.pagos),0)*1 as pagos,
coalesce(sum(cc.saldo_cuenta),0)*1 as saldo 
FROM 
cuentas_cobro cc 
INNER JOIN estados_cartera ec ON (ec.codigo=cc.estado) 
INNER JOIN convenios c ON (c.codigo=cc.convenio) 
INNER JOIN empresas e ON (e.codigo=c.empresa) 
INNER JOIN terceros t ON (t.codigo=e.tercero) 
where 1=1
group by t.descripcion, c.nombre, ec.descripcion 
ORDER BY t.descripcion, c.nombre, ec.descripcion</property>
