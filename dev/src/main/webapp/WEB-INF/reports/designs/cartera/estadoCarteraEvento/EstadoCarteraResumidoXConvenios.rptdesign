            <property name="queryText">SELECT 
	'Generada' AS estado, 
	coalesce(sum(cc.valor_inicial_cuenta),0)*1 as totalvalorinicial, 
	coalesce(sum(cc.ajuste_debito),0)*1 as totalajustedebito, 
	coalesce(sum(cc.ajuste_credito),0)*1 as totalajustecredito, 
	coalesce(sum(cc.pagos),0)*1 as totalpagos, 
	coalesce(sum(cc.saldo_cuenta),0)*1 as totalsaldo 
FROM 
	cuentas_cobro cc 
	INNER JOIN estados_cartera ec ON (ec.codigo=cc.estado) 
	INNER JOIN convenios c ON (c.codigo=cc.convenio) 
	INNER JOIN empresas e ON (e.codigo=c.empresa) 
	INNER JOIN terceros t ON (t.codigo=e.tercero) 
WHERE 
	cc.estado=1 and 1=1
UNION SELECT 
	'Radicada' AS estado, 
	coalesce(sum(cc.valor_inicial_cuenta),0)*1 as totalvalorinicial, 
	coalesce(sum(cc.ajuste_debito),0)*1 as totalajustedebito, 
	coalesce(sum(cc.ajuste_credito),0)*1 as totalajustecredito, 
	coalesce(sum(cc.pagos),0)*1 as totalpagos, 
	coalesce(sum(cc.saldo_cuenta),0)*1 as totalsaldo 
FROM 
	cuentas_cobro cc 
	INNER JOIN estados_cartera ec ON (ec.codigo=cc.estado) 
	INNER JOIN convenios c ON (c.codigo=cc.convenio) 
	INNER JOIN empresas e ON (e.codigo=c.empresa) 
	INNER JOIN terceros t ON (t.codigo=e.tercero) 
WHERE 
	cc.estado=4 and 1=1
UNION SELECT 
	'Anulada' AS estado, 
	coalesce(sum(cc.valor_inicial_cuenta),0)*1 as totalvalorinicial, 
	coalesce(sum(cc.ajuste_debito),0)*1 as totalajustedebito, 
	coalesce(sum(cc.ajuste_credito),0)*1 as totalajustecredito, 
	coalesce(sum(cc.pagos),0)*1 as totalpagos, 
	coalesce(sum(cc.saldo_cuenta),0)*1 as totalsaldo 
FROM 
	cuentas_cobro cc 
	INNER JOIN estados_cartera ec ON (ec.codigo=cc.estado) 
	INNER JOIN convenios c ON (c.codigo=cc.convenio) 
	INNER JOIN empresas e ON (e.codigo=c.empresa) 
	INNER JOIN terceros t ON (t.codigo=e.tercero) 
WHERE cc.estado=2 and 1=1</property>
