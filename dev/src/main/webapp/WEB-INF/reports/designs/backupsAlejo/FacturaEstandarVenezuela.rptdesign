                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">telefonoresponsable</property>
                    <property name="nativeName">telefonoresponsable</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT
	f.codigo AS factura,
	f.convenio AS convenio,
	c.tipo_regimen AS tipoconvenio,
	CASE WHEN c.tipo_regimen = 'P' THEN (CASE WHEN cu.codigo_responsable_paciente IS NOT NULL THEN rp.primer_nombre || ' ' || rp.primer_apellido ELSE p.primer_nombre || ' ' || p.primer_apellido END) ELSE c.nombre END AS nombreresponsable,
	CASE WHEN c.tipo_regimen = 'P' THEN (CASE WHEN cu.codigo_responsable_paciente IS NOT NULL THEN rp.tipo_identificacion || ' ' || rp.numero_identificacion ELSE p.tipo_identificacion || ' ' || p.numero_identificacion END) ELSE t.numero_identificacion END AS idresponsable,
	CASE WHEN c.tipo_regimen = 'P' THEN (CASE WHEN cu.codigo_responsable_paciente IS NOT NULL THEN rp.direccion ELSE p.direccion END) ELSE e.direccion END AS direccionresponsable,
	CASE WHEN c.tipo_regimen = 'P' THEN (CASE WHEN cu.codigo_responsable_paciente IS NOT NULL THEN rp.telefono ELSE p.telefono END) ELSE e.telefono END AS telefonoresponsable
 FROM
	facturas f
	LEFT OUTER JOIN convenios c ON (f.convenio = c.codigo)
	LEFT OUTER JOIN cuentas cu ON (f.cuenta = cu.id)
	LEFT OUTER JOIN responsables_pacientes rp ON (cu.codigo_responsable_paciente = rp.codigo)
	LEFT OUTER JOIN empresas e ON (c.empresa = e.codigo)
	LEFT OUTER JOIN terceros t ON (e.tercero = t.codigo)
	LEFT OUTER JOIN personas p ON (cu.codigo_paciente = p.codigo)
WHERE
	1=2</property>
