            <property name="queryText">SELECT
	f.codigo AS codigofactura,
	'No. de Factura: ' || f.consecutivo_factura AS consecutivofactura,
	p.primer_apellido || ' ' || p.segundo_apellido || ' ' || p.primer_nombre || ' ' || p.segundo_nombre AS nombrepaciente,
	to_char(f.fecha, 'DD/MM/YYYY') || ' - ' || to_char(f.hora, 'HH24:MM') AS fechafactura,
	p.tipo_identificacion AS tipoid,
	p.numero_identificacion AS numeroid,
	p.direccion AS direccion,
	p.telefono AS telefono,
	f.convenio AS convenio,
	CASE WHEN f.estado_facturacion = 2 THEN 'Anulada' ELSE '' END AS estadofactura,
	f.estado_facturacion AS estado
FROM
	facturas f
	INNER JOIN personas p ON (f.cod_paciente = p.codigo)
WHERE
	1=2</property>
