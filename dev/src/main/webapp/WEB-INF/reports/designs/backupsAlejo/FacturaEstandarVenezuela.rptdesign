            <property name="queryText">SELECT
	f.valor_total AS valortotal,
    CASE WHEN c.tipo_regimen = 'P' THEN 'NO APLICA' ELSE '' END AS textovalorconvenio,
	CASE WHEN c.tipo_regimen = 'P' THEN -1 ELSE f.valor_convenio END AS valorconvenio,
    f.valor_bruto_pac AS valorpaciente,
    f.valor_abonos AS valorabonos,
    f.val_desc_pac AS valordescuento,
    f.valor_neto_paciente AS valornetopaciente,
    CASE WHEN f.valor_neto_paciente >= 0 THEN 'Valor Neto a Cargo del Paciente' ELSE 'Valor Neto a Favor del Paciente' END AS textonetopaciente,
	convertirletras(f.valor_total::numeric) AS valorletras,
	f.convenio AS convenio,
	getnombreconvenio(convenio) AS nombreconvenio,
	c.tipo_regimen AS tiporegimen
FROM
	facturas f
	INNER JOIN convenios c ON (f.convenio = c.codigo)
WHERE
    1=2</property>
