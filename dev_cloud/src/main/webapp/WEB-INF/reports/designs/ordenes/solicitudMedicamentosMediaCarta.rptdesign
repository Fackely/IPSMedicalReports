        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="encabezado" id="49">
            <structure name="cachedMetaData"/>
            <property name="dataSource">axioma</property>
            <list-property name="parameters">
                <structure>
                    <property name="name">numeroSolicitud</property>
                    <property name="paramName">numeroSolicitud</property>
                    <property name="dataType">integer</property>
                    <property name="position">1</property>
                    <expression name="defaultValue">0</expression>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
            </list-property>
            <property name="queryText">select
	'Paciente: '|| getnombrepersona(c.codigo_paciente) ||', '||p.tipo_identificacion ||' '|| p.numero_identificacion||', No Ingreso: '||getconsecutivoingreso(c.id_ingreso)||', No Cuenta: '||c.id as nombre_paciente,
	'Edad: '|| getedad2(p.fecha_nacimiento, s.fecha_solicitud)||', Nro.Historia Clínica: '||p.tipo_identificacion ||' - '|| p.numero_identificacion ||', Tel: '|| coalesce(p.telefono, '') as info_pac,
	'Nro.Orden: '||s.consecutivo_ordenes_medicas ||', Fecha-Hora: '|| to_char(s.fecha_solicitud, 'DD/MM/YYYY') ||' '|| s.hora_solicitud as info_orden,
	'Lugar de Prescripción: '||getnomcentroatencion(s.centro_costo_solicitante) as lugar_prescripcion,
	'Profesional que Pres-escribe: '|| getnombremedico(s.codigo_medico) ||', Especialidad: '||getnombreespecialidad(s.especialidad_solicitante) as info_medico
from
	solicitudes s
	inner join cuentas c on(c.id=s.cuenta)
	inner join personas p on(p.codigo= c.codigo_paciente)
where
	s.numero_solicitud=?</property>
        </oda-data-set>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="medicamentos" id="69">
            <structure name="cachedMetaData"/>
            <property name="dataSource">axioma</property>
            <list-property name="parameters">
                <structure>
                    <property name="name">numeroSolicitud</property>
                    <property name="paramName">numeroSolicitud</property>
                    <property name="dataType">integer</property>
                    <property name="position">1</property>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT
ds.articulo as articulo,
getCodArticuloAxiomaInterfaz(ds.articulo, 'AMBOS') as codarticulo,
getdescarticulo(ds.articulo) as descarticulo,
<!-- se cambia esta linea por tarea 29027 no mostrar la cantidad de unidosis en el campo dosis
ds.dosis ||' '|| uxa.unidad_medida ||' - '|| CASE WHEN uxa.cantidad IS NULL THEN '' ELSE uxa.cantidad||'' END as dosis,-->
ds.dosis ||' '|| uxa.unidad_medida as dosis,
'Cada '||ds.frecuencia ||' '||ds.tipo_frecuencia as frecuencia,
ds.via as nomvia,
ds.cantidad||' ('||convertirletras(ds.cantidad)||')' as ca,
ds.dias_tratamiento as duracion,
ds.observaciones as observaciones,
ds.unidosis_articulo as unidosis
from
detalle_solicitudes ds
left outer join unidosis_x_articulo uxa on (ds.unidosis_articulo=uxa.codigo)
where
ds.numero_solicitud=? and getesmedicamento(ds.articulo)='1'</property>
        </oda-data-set>
